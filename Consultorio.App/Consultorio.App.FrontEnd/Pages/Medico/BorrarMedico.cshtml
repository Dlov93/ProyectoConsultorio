@page
@model MyApp.Namespace.BorrarMedicoModel
@{
}
<h1>Confirmación</h1>
<p>Desea eliminar al médido @Model.medico.Nombre ?</p>
<form method="post" class="mt-3">
    <input type="hidden" asp-for="medico.Documento"/>
    <button type="submit" class="btn btn-primary">Si</button>
        <a asp-page="./Medico" class="btn btn-primary">No</a>
</form>
