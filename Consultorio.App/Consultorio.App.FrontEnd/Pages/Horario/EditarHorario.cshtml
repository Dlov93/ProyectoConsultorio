@page
@model MyApp.Namespace.EditarHorarioModel
@{
}
@using Consultorio.App.Dominio

<form method="post" class="mt-3">
    <input hidden asp-for="horario.ID"/>
    <div class="form-group row">
        <label asp-for="horario.NameH" class="col-sm-2"></label>
        <input asp-for="horario.NameH" class="form-control">
    </div>
    <div class="form-group row">
        <label asp-for="horario.FechaDisponible" class="col-sm-2"></label>
        <input asp-for="horario.FechaDisponible" class="form-control">
    </div>
    <div class="form-group row">
        <button type="submit" class="btn btn-primary">Actualizar</button>
        <a asp-page="./Horario" class="btn btn-primary">Cancelar</a>
    </div>
</form>