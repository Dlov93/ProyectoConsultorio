@page
@model MyApp.Namespace.BorrarHorarioModel
@{
}
<h1>Confirmación</h1>
<p>¿ Realmente desea eliminar el horario @Model.horario.NameH ?</p>
<form method="post" class="mt-3">
    <input type="hidden" asp-for="horario.NameH"/>
    <button type="submit" class="btn btn-primary">Si</button>
    <a asp-page="./Horario" class="btn btn-primary">No</a>
</form>